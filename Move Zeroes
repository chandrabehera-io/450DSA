class Solution {
    public void moveZeroes(int[] nums) {
		int sizeofarr=nums.length;
		int count=0;
        for(int i=0;i<sizeofarr;i++){
            if(nums[i]!=0){
                nums[count++]=nums[i];
            }
        }
    while(count<sizeofarr){
        nums[count++]=0;
    }
    for(int j=0;j<sizeofarr;j++){
    System.out.print(nums[j]+" ");
    }
}
}
